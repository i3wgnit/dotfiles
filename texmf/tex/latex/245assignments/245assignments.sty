  \ifnum\ccxlv@wd@i<\ccxlv@wd@iii\setlength{\ccxlv@wd@i}{\ccxlv@wd@iii}\fi
  \ensuremath{\genfrac{}{}{0pt}{0}%
    %\vbox
    {\makebox[\ccxlv@wd@i][l]{\hoarebegin{#1}}}%
    {\genfrac{}{}{0pt}{0}%
      {\makebox[\ccxlv@wd@i][c]{\hoaremid{#2}}}%
